        },
    "MENU_MEDIA": {
      "description": "Add url for menu image/video.",
      "required": true,
      "value": "https://i.imgur.com/9WOLsb9.png"
    },
    "AUTHOR": {
      "description": "prefix for commands, enter false for no prefix",
